[Context]
filesystems=!xdg-run/app/com.discordapp.Discord;
unset-environment=SDL_VIDEODRIVER;SDL_AUDIODRIVER;SDL_AUDIO_DRIVER;SDL_VIDEO_DRIVER;
sockets=session-bus;

[Session Bus Policy]
org.freedesktop.UPower=none
org.freedesktop.ScreenSaver=none
org.gnome.SettingsDaemon.MediaKeys=none
org.freedesktop.PowerManagement=none
org.freedesktop.UDisks2=none

[Environment]
SDL_VIDEODRIVER=
SDL_AUDIODRIVER=
SDL_AUDIO_DRIVER=
SDL_VIDEO_DRIVER=
