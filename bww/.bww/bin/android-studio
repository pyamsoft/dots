#!/bin/sh

# This file is autogenerated by bwrapper, modifications may be
# overwritten by the script.

# This script expects the 'bww' script to be available to the user somewhere
# on the $PATH. It will fail if the situation proves otherwise.

readonly arguments=" \
  --nopid \
  --nodev \
  --bind-if-exists=/storage/pyamsoft/devel/android \
  --setenv PATH /storage/pyamsoft/devel/android/sdk/platform-tools:/storage/pyamsoft/devel/android/sdk/tools:/opt/android-studio/jre/bin:/opt/android-studio/jre/jre/bin:${PATH} \
  "


if [ "$1" = "shell" ]; then
  # shellcheck disable=SC2086
  shift
  exec bww $arguments --shell android-studio "$@"
else
  # shellcheck disable=SC2086
  exec bww $arguments android-studio "$@"
fi
