# No Color
colorOff="\033[0m"

# Normal colors
# Format color<COLOR>
colorBl="\033[0;30m"
colorR="\033[0;31m"
colorG="\033[0;32m"
colorY="\033[0;33m"
colorB="\033[0;34m"
colorP="\033[0;35m"
colorC="\033[0;36m"
colorW="\033[0;37m"

# Bold Colors
# Format Bcolor<COLOR>
BcolorBl="\033[1;30m"
BcolorR="\033[1;31m"
BcolorG="\033[1;32m"
BcolorY="\033[1;33m"
BcolorB="\033[1;34m"
BcolorP="\033[1;35m"
BcolorC="\033[1;36m"
BcolorW="\033[1;37m"

# Underlined colors
# Format Ucolor<COLOR>
UcolorBl="\033[4;30m"
UcolorR="\033[4;31m"
UcolorG="\033[4;32m"
UcolorY="\033[4;33m"
UcolorB="\033[4;34m"
UcolorP="\033[4;35m"
UcolorC="\033[4;36m"
UcolorW="\033[4;37m"

