# shellcheck shell=bash

if [ -f "/usr/share/bash-complete-alias/complete_alias" ]; then
  . "/usr/share/bash-complete-alias/complete_alias"

  complete -F _complete_alias gits
  complete -F _complete_alias gita
  complete -F _complete_alias gitc
  complete -F _complete_alias gitd
  complete -F _complete_alias gitm
  complete -F _complete_alias gitck
  complete -F _complete_alias gitb
  complete -F _complete_alias gitpl
  complete -F _complete_alias gitf
  complete -F _complete_alias gitps
  complete -F _complete_alias gitl
fi
