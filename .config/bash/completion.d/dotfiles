# shellcheck shell=bash

if [ -f "/usr/share/bash-complete-alias/complete_alias" ]; then
  . "/usr/share/bash-complete-alias/complete_alias"

  complete -F _complete_alias dotfiles

  complete -F _complete_alias dots
  complete -F _complete_alias dota
  complete -F _complete_alias dotc
  complete -F _complete_alias dotd
  complete -F _complete_alias dotm
  complete -F _complete_alias dotck
  complete -F _complete_alias dotb
  complete -F _complete_alias dotpl
  complete -F _complete_alias dotf
  complete -F _complete_alias dotps
  complete -F _complete_alias dotl
fi
