# shellcheck shell=bash

if ! declare -f _load_completion_from >/dev/null 2>&1; then
  # Load an existing completion file
  _load_completion_from() {
    if declare -f _comp_load >/dev/null 2>&1; then
      _comp_load "$@" || return 1
    else
      __load_completion "$@" || return 1
    fi

    return 0
  }
fi

# vim: set filetype=sh:
